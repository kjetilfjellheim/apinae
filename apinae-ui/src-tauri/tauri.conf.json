{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "apinae-ui",
  "version": "0.1.14",
  "identifier": "net.forgottendonkey.apinae",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,    
    "windows": [
      {
        "title": "apinae-ui",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null      
    }               
  },  
  "bundle": {    
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}